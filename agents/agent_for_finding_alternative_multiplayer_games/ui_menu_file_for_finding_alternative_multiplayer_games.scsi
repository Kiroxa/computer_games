ui_menu_file_for_finding_alternative_multiplayer_games
	<- 	ui_user_command_class_atom; 
		ui_user_command_class_view_kb;
	=> nrel_main_idtf: 
		[Найти альтернативные многопользовательские игры](* <- lang_ru;; *);
		[Find alternative multiplayer games] (* <- lang_en;; *);
	=> ui_nrel_command_template:
		[*
		question_finding_alternative_multiplayer_games _-> ._question_finding_alternative_multiplayer_games_instance
			(*
			_-> ui_arg_1;;
			*);;
		._question_finding_alternative_multiplayer_games_instance _<- question;;
		*];
	=> ui_nrel_command_lang_template: 
		[Запрос поиска альтернативных игр для: $ui_arg_1] 
		(* <- lang_ru;; *);
		[Request of finding alternative games for: $ui_arg_1] 
		(* <- lang_en;; *);;

