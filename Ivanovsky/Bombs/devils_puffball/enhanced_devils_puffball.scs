      [Утв.(Эффект использования)] (* <- lang_ru;; *);;
     <= nrel_sc_text_translation: ... (*
          -> [При взрыве испускает ядовитое облако. Действует сильнее.](*<-lang_ru;;*);;
      *);;
  *);

<- rrel_key_sc_element: ... (*
    <- explanation;;
	<= nrel_using_constants: ...(*
	-> concept_calcium_equum;;
	-> concept_ginatia_petals;;
	-> concept_stammelfords_dust;;
	-> concept_ordinary_devils_puffball;;
   	-> concept_szytnacce;;
   	-> concept_endrega_heart;;
   	-> concept_green_mold;;
		*);;
    => nrel_main_idtf:
      [Утв.(Состав)] (* <- lang_ru;; *);;
